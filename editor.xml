<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="PRESELECT_RENAME" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="false" pattern="\bmfhack\b.*">
      <option name="FOREGROUND" value="ff0300" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ffffff" />
      <option name="ERROR_STRIPE_COLOR" value="ff0300" />
      <option name="EFFECT_TYPE" value="2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="false" pattern="\bnocommit\b.*">
      <option name="FOREGROUND" value="ff0300" />
      <option name="BACKGROUND" value="fffa00" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="fffa00" />
      <option name="ERROR_STRIPE_COLOR" value="ff0300" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bmftofix\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bmftodo\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bjpatodo\b.*">
      <option name="FOREGROUND" value="ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <filter name="nocommit">
      <pattern index="5" />
      <pattern index="4" />
      <pattern index="3" />
    </filter>
  </component>
</application>